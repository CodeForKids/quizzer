<div class="row-fluid">
  <div class="span12">
    <hr>
    <div class="result aggregatable">
      <h4>Question: <%- resultObj.question_text %></h4>
      <div class="results-chart">
        <canvas width="200" height="200" class="pull-left"></canvas>
        <div class="span1"></div>
        <div class="pull-left">
          <ul style="list-style: none">
            <h6>Legend</h6>
            <hr>
            <% $.each(legendData, function(index, legend){ %>
              <li>
                <div class="pull-left legend" style="background: <%- legend.color %>"></div>
                <%- legend.key %>
              </li>
            <% }); %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="clearfix"></div>
