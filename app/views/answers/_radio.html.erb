<%= render partial: "answers/errors", locals: {answer: answer} %>
<%= render partial: "answers/title", locals: {answer: answer, form: f} %>
<%- answer.question.options.each do |option| %>
  <%= f.label "answer_text_#{option}", option do %>
    <%= f.radio_button :answer_text, option %>
    <%= option %>
  <% end %>
<% end %>
